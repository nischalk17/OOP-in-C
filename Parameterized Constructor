// A constructor that accepts parameter is called parameterized constructor


#include<iostream>
using namespace std;
class A
{
	public:
	int a,b;
		A(int x, int y)		//parameterized constructor
		{
			a=x;
			b=y;
		}
		void show()
		{
				cout<<"a= "<<a<<endl<<"b= "<<b<<endl;
		}
};
int main()
{
	A obj(20,30);
	obj.show();
	return 0;
}

/*Output:
a= 20
b= 30
*/
