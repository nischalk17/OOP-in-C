// A constructor that accepts no parameter is called default constructor


#include<iostream>
using namespace std;
class A
{
	public:
	int a,b;
		A()		//default constructor
		{
			a=20;
			b=30;
		}
		void show()
		{
			cout<<"a= "<<a<<endl<<"b= "<<b<<endl;
		}
};
int main()
{
	A obj;
	obj.show();
	return 0;
}

/*Output:
a= 20
b= 30
*/
