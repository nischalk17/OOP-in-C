// A constructor that accepts no parameter is called default constructor


#include<iostream>
using namespace std;
class A
{
	public:
	int a,b;
		A()		//default constructor
		{
			a=20;
			b=30;
		}
};
int main()
{
	A obj;
	cout<<"a= "<<obj.a<<endl<<"b= "<<obj.b<<endl;
	return 0;
}

/*Output:
a= 20
b= 30
*/
